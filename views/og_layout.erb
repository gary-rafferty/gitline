<html>
  <head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# gitline: http://ogp.me/ns/fb/gitline#">
    <meta property="fb:app_id" content="402453166505117" /> 
    <meta property="og:type" content="gitline:commit" /> 
    <meta property="og:url" content="http://gitline.herokuapp.com/commits/<%= @payload.id %>" />
    <meta property="og:title" content="<%= @payload.pretty_commit %>" /> 
    <meta property="og:description" content="<%= @payload.commits.first['message']%>" /> 
    <meta property="og:repository" content="<%= @payload.repository.id %>" /> 
    <meta property="og:image" content="https://raw.github.com/gary-rafferty/gitline/master/public/images/155x100.png" />
  </head>
</html>
